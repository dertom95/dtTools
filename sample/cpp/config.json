{
    "name" : "cpp-generator",
    "rootname": "root",
    "config" : {
        "xsd-schema-name":"dtData",
        "xsd-output":"${configfolder}/gen/xsd/dtData.xsd",
        "gen-input-file":["${configfolder}/simple_inputdata.xml"],
        "gen-force-overwrite":true,
        "gen-root-folder":"${configfolder}/gen/output"
    },
    "templates": [
        {
            "name": "cpp-header",
            "path": "templates/simple_cpp.h"
        },
        {
            "name": "cpp-body",
            "path": "templates/simple_cpp.cpp"
        }
    ]
}