{
    "name" : "cpp-generator",
    "rootname": "root",
    "config" : {
        "xsd-schema-name":"dtData",
        "xsd-output":"${configfolder}/gen/xsd/dtData.xsd",
        "gen-input-folder":["${configfolder}/input"],
        "gen-force-overwrite":true,
        "gen-root-folder":"${configfolder}/gen/output",
        "start-runtime":true
    },
    "templates": [
        {
            "name": "cpp-header",
            "path": "${configfolder}/templates/simple_cpp.h"
        },
        {
            "name": "cpp-body",
            "path": "${configfolder}/templates/simple_cpp.cpp"
        }
    ]
}